{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.15.3"
  },
  "main": "main.js",
  "license": "GPL-3.0",
  "scripts": {
    "start": "node app.js",
    "dev": "react-scripts start",
    "electron": "electron .",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "cypress:open": "npx cypress open",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "format": "prettier --write {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "format:ci": "prettier --list-different {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "codecov": "codecov -t 187fc664-9fb9-4a79-9064-c7e6d96fc023",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts"
    ]
  },
  "dependencies": {
    "blueimp-md5": "^2.10.0",
    "bulma": "^0.7.4",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "diff": "4.0.1",
    "express": "4.17.1",
    "express-sslify": "1.2.0",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "react-use": "^9.1.0",
    "react-virtualized": "9.21.0",
    "serve-static": "1.14.1",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@types/blueimp-md5": "^2.7.0",
    "@types/diff": "4.0.2",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/node": "12.0.9",
    "@types/react": "^16.8.13",
    "@types/react-dom": "16.8.4",
    "@types/react-router-dom": "4.3.4",
    "@types/react-virtualized": "^9.21.1",
    "@types/socket.io-client": "1.4.32",
    "codecov": "3.5.0",
    "cypress": "3.3.2",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.1",
    "react-testing-library": "6.1.2",
    "source-map-explorer": "^2.0.0",
    "tslint": "5.12.1",
    "tslint-react-hooks": "^2.0.0",
    "typescript": "3.3.3"
  }
}
