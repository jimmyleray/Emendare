{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "node app.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "cypress:open": "npx cypress open",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "format": "prettier --write {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "format:ci": "prettier --list-different {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "codecov": "codecov -t 187fc664-9fb9-4a79-9064-c7e6d96fc023",
    "doc": "typedoc --out ./docs --name Emendare --readme ../README.md --target ES6 --downlevelIteration ./src",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts"
    ]
  },
  "dependencies": {
    "bulma": "^0.7.4",
    "chart.js": "2.7.3",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "diff": "4.0.1",
    "express": "4.16.4",
    "express-sslify": "1.2.0",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-chartjs-2": "2.7.5",
    "react-dom": "16.8.6",
    "react-json-tree": "^0.11.2",
    "react-router-dom": "4.4.0",
    "react-scripts": "^2.1.8",
    "react-use": "^5.15.0",
    "react-virtualized": "9.21.0",
    "serve-static": "1.13.2",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@types/chart.js": "2.7.44",
    "@types/diff": "4.0.2",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "24.0.11",
    "@types/lodash": "^4.14.122",
    "@types/node": "11.11.8",
    "@types/react": "16.8.10",
    "@types/react-dom": "16.8.3",
    "@types/react-json-tree": "^0.6.11",
    "@types/react-router-dom": "4.3.1",
    "@types/socket.io-client": "1.4.32",
    "@types/react-virtualized": "9.21.0",
    "codecov": "3.1.0",
    "cypress": "3.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "husky": "1.3.1",
    "lint-staged": "8.1.5",
    "prettier": "1.16.4",
    "react-docgen-typescript": "^1.12.3",
    "react-styleguidist": "^9.0.1",
    "tslint": "5.12.1",
    "tslint-react-hooks": "^2.0.0",
    "typedoc": "0.14.2",
    "typescript": "3.3.3",
    "react-testing-library": "6.0.2"
  }
}
