{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=8.3.0"
  },
  "license": "GPL-3.0",
  "scripts": {
    "start": "node app.js",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage",
    "cypress:open": "npx cypress open",
    "eject": "react-scripts eject",
    "lint": "tslint -c tslint.json src/**/*.ts",
    "format": "prettier --write {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "format:ci": "prettier --list-different {src,public,cypress}/**/*.{ts,tsx,js,jsx,css,scss,json,md}",
    "codecov": "codecov -t 187fc664-9fb9-4a79-9064-c7e6d96fc023",
    "doc": "typedoc --out ./docs --name Emendare --readme ../README.md --target ES6 --downlevelIteration ./src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "browserslist": [
    "> 5%"
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts"
    ]
  },
  "dependencies": {
    "chart.js": "2.7.3",
    "compression": "1.7.3",
    "cors": "2.8.5",
    "diff": "4.0.1",
    "express": "4.16.4",
    "express-sslify": "1.2.0",
    "lodash": "4.17.11",
    "react": "16.8.1",
    "react-chartjs-2": "2.7.4",
    "react-dom": "16.8.1",
    "react-router-dom": "4.3.1",
    "react-scripts": "2.1.3",
    "socket.io-client": "2.2.0"
  },
  "devDependencies": {
    "@types/chart.js": "2.7.43",
    "@types/diff": "4.0.1",
    "@types/enzyme": "^3.1.17",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "24.0.0",
    "@types/node": "10.12.22",
    "@types/react": "16.8.2",
    "@types/react-dom": "16.8.0",
    "@types/react-router-dom": "4.3.1",
    "@types/socket.io-client": "1.4.32",
    "babel-eslint": "9.0.0",
    "codecov": "3.1.0",
    "cypress": "3.1.5",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "eslint-config-prettier": "4.0.0",
    "eslint-config-react-app": "3.0.6",
    "eslint-plugin-chai-friendly": "0.4.1",
    "eslint-plugin-compat": "2.6.3",
    "eslint-plugin-cypress": "2.2.0",
    "eslint-plugin-flowtype": "3.2.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.0.1",
    "eslint-plugin-react": "7.12.4",
    "eslint-plugin-sonarjs": "0.2.0",
    "husky": "1.3.1",
    "lint-staged": "8.1.3",
    "prettier": "1.16.4",
    "tslint": "5.12.1",
    "typedoc": "0.14.2",
    "typescript": "3.3.3"
  }
}
